#domain
  %s == "register.com"

#domain_id
  %s == ""


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{1994-11-01T00:00:00-0500}

#updated_on
  %s %CLASS{time}
  %s %TIME{2009-08-26T00:00:00-0400}

#expires_on
  %s %CLASS{time}
  %s %TIME{2019-08-04T00:00:00-0400}


#registrar
  %s %CLASS{registrar}
  %s.id           == "9"
  %s.name         == "Register.com"
  %s.url          == "http://www.register.com"


#registrant_contacts
  %s %CLASS{array}
  %s.size eq(1)
  %s[0] %CLASS{contact}
  %s[0].type         == Whois::Record::Contact::TYPE_REGISTRANT
  %s[0].name         == "Domain Registrar"
  %s[0].organization == "Register.Com, Inc."
  %s[0].address      == "575 8th Avenue"
  %s[0].city         == "New York"
  %s[0].zip          == "10018"
  %s[0].state        == "NY"
  %s[0].country_code == "US"
  %s[0].phone        == "+1.9027492701"
  %s[0].fax          == ""
  %s[0].email        == "domainregistrar@register.com"

