#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2000-09-11}

#updated_on
  %s %CLASS{time}
  %s %TIME{2000-09-11}

#expires_on
  %s %ERROR{AttributeNotSupported}


#nameservers
  %s %CLASS{array}
  %s.size eq(2)
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.peer1.net"
  %s[0].ipv4 == nil
  %s[0].ipv6 == nil
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.peer1.net"
  %s[1].ipv4 == nil
  %s[1].ipv6 == nil
