#domain
  %s == "tucows.com"

#domain_id
  %s == ""


#status
  %s == :registered


#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{1995-09-07 04:00:00}

#updated_on
  %s %CLASS{time}
