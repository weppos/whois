#domain
  %s == "wildwestdomains.com"

#domain_id
  %s == nil


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2000-08-22 18:29:11}

#updated_on
  %s %CLASS{time}
  %s %TIME{2011-11-01 16:31:47}

#expires_on
  %s %CLASS{time}
  %s %TIME{2021-11-01 06:59:59}


#registrar
  %s %CLASS{registrar}
  %s.id           == nil
  %s.name         == "Wild West Domains, LLC"
  %s.organization == "Wild West Domains, LLC"
  %s.url          == "http://www.wildwestdomains.com"


