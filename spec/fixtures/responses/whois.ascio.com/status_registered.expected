#domain
  %s == "ascio.com"

#domain_id
  %s == ""


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2005-03-01 14:11:50 UTC}

#updated_on
  %s %CLASS{time}
  %s %TIME{2013-06-02 00:23:21 UTC}

#expires_on
