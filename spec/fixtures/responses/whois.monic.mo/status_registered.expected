  %s %ERROR{AttributeNotSupported}


#nameservers
  %s %CLASS{array}
  %s.size eq(2)
  %s[0] %CLASS{nameserver}
  %s[0].name == "umacsn1.umac.mo"
  %s[1] %CLASS{nameserver}
  %s[1].name == "umacsn2.umac.mo"
