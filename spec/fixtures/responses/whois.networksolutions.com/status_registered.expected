#domain
  %s == "xif.com"

#domain_id
  %s == ""


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{1997-06-10}

#updated_on
  %s %CLASS{time}
  %s %TIME{2012-02-13}

#expires_on
  %s %CLASS{time}
  %s %TIME{2014-06-09}


#registrar
  %s %CLASS{registrar}
  %s.id           == "2"
  %s.name         == "NETWORK SOLUTIONS, LLC."
  %s.organization == "NETWORK SOLUTIONS, LLC."
  %s.url          == "http//www.networksolutions.com/en_US/"


