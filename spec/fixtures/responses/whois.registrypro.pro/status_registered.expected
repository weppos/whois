#domain
  %s == "google.pro"

#domain_id
  %s == "D11545-PRO"


#status
  %s == ["CLIENT DELETE PROHIBITED", "CLIENT TRANSFER PROHIBITED", "CLIENT UPDATE PROHIBITED"]

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2008-07-22 00:00:00 UTC}

#updated_on
  %s %CLASS{time}
  %s %TIME{2013-08-07 09:18:18 UTC}

#expires_on
  %s %CLASS{time}
  %s %TIME{2014-09-08 00:00:00 UTC}


#registrar
