#status
  should: %s == :inactive

#available?
  should: %s == false

#registered?
  should: %s == true


#created_on
  should: %s CLASS(time)
  should: %s == Time.parse("2009-02-09 00:00:00 UTC")

#updated_on
  should: %s raise_error(Whois::PropertyNotSupported)

#expires_on
  should: %s CLASS(time)
  should: %s == Time.parse("2014-03-01 00:00:00 UTC")


#nameservers
  should: %s CLASS(array)
  should: %s have(0).items
