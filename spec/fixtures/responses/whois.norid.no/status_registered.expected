#status
  should: %s == :registered

#available?
  should: %s == false

#registered?
  should: %s == true


#created_on
  should: %s CLASS(time)
  should: %s == Time.parse("2001-02-26")

#updated_on
  should: %s CLASS(time)
  should: %s == Time.parse("2006-12-07")

#expires_on
  should: %s raise_error(Whois::PropertyNotSupported)
