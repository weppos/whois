#domain
  %s == "yoursrs.com"

#domain_id
  %s %ERROR{AttributeNotSupported}


#status
  %s == ["OK"]

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2003-11-24 09:40:55}

#updated_on
  %s %CLASS{time}
  %s %TIME{2014-01-22 14:04:50}

#expires_on
  %s %CLASS{time}
