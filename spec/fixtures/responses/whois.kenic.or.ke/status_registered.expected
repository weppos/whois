#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2003-04-18 00:00:00 EAT}

#updated_on
  %s %CLASS{time}
  %s %TIME{2013-12-16 12:48:00 EAT}

#expires_on
  %s %CLASS{time}
  %s %TIME{2015-01-01 00:00:00 EAT}


#nameservers
  %s %CLASS{array}
  %s.size eq(4)
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.google.com"
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.google.com"
  %s[2] %CLASS{nameserver}
  %s[2].name == "ns3.google.com"
  %s[3] %CLASS{nameserver}
  %s[3].name == "ns4.google.com"
