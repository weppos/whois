#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2005-06-25 16:03:30}

#updated_on
  %s %ERROR{AttributeNotSupported}

#expires_on
  %s %CLASS{time}
  %s %TIME{2014-06-25}


#nameservers
  %s %CLASS{array}
  %s.size eq(2)
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.canadawebhosting.com"
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.canadawebhosting.com"


#reserved?
  %s be_falsey
