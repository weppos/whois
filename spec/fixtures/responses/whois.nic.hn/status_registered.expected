#domain
  %s == "google.hn"

#domain_id
  %s == "801220-CoCCA"


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2003-03-07 05:00:00 UTC}

#updated_on
  %s %CLASS{time}
  %s %TIME{2014-02-03 10:22:23 UTC}

#expires_on
  %s %CLASS{time}
  %s %TIME{2015-03-07 05:00:00 UTC}


#registrar
  %s %CLASS{registrar}
  %s.id           == nil
  %s.name         == "MarkMonitor"
  %s.url          == "http://www.markmonitor.com"


#nameservers
  %s %CLASS{array}
  %s.size eq(2)
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.google.com"
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.google.com"
