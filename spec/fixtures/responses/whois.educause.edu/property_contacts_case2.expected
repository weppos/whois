  %s[0].organization  == nil
  %s[0].address       == "241 Panama Street Pine Hall, Room 115"
  %s[0].city          == "Stanford"
  %s[0].zip           == "94305-4122"
  %s[0].state         == "CA"
  %s[0].country       == "UNITED STATES"
  %s[0].country_code  == nil
  %s[0].phone         == "(650) 723-4328"
  %s[0].fax           == nil
  %s[0].email         == "sunet-admin@stanford.edu"
