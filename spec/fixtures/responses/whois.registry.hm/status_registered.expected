#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2003-04-28}

#updated_on
  %s %ERROR{AttributeNotSupported}

#expires_on
  %s %CLASS{time}
  %s %TIME{2020-04-28}


#nameservers
  %s %CLASS{array}
  %s.size eq(2)
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.everydns.net"
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.everydns.net"
