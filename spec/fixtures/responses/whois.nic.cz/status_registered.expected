#domain
  %s == "google.cz"

#domain_id
  %s %ERROR{AttributeNotSupported}


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2000-07-21 15:21:00}

#updated_on
  %s %CLASS{time}
  %s %TIME{2011-05-18 23:28:45}

#expires_on
  %s %CLASS{time}
  %s %TIME{2014-07-22}


#registrar
