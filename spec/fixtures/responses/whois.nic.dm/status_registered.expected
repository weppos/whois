#domain
  %s == "google.dm"

#domain_id
  %s %ERROR{AttributeNotSupported}


#status
  %s == :registered

#available?
  %s be_falsey

#registered?
  %s be_truthy


#created_on
  %s %CLASS{time}
  %s %TIME{2004-08-23 23:00:00}

#updated_on
  %s %CLASS{time}
  %s %TIME{2013-07-23 17:50:34}

#expires_on
  %s %CLASS{time}
  %s %TIME{2014-08-23 23:00:00}


#registrar
  %s %CLASS{registrar}
  %s.id           == nil
  %s.name         == "MarkMonitor Inc."
  %s.organization == nil
  %s.url          == "www.markmonitor.com"

